<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="edu.jyu.erp.invoice.orderdetail.vo.OrderDetailModel" table="tbl_orderDetail">
        <id name="uuid">
            <generator class="native" />
        </id>
        <property name="num"/>
        <property name="surplus"/>
        <property name="price"/>
        <many-to-one 
        	name="gm"
        	column="goodsUuid"
        	class="edu.jyu.erp.invoice.goods.vo.GoodsModel"
        	/>
        <many-to-one 
        	name="om"
        	column="orderUuid"
        	class="edu.jyu.erp.invoice.order.vo.OrderModel"
        	/>
    </class>
</hibernate-mapping>
